{
  "name": "fpt-test-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn watch:graphql & next dev",
    "start:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json,css,scss,less}\" --config ./.prettierrc",
    "gen:graphql": "DOTENV_CONFIG_PATH=.env.production graphql-codegen --require dotenv/config --config codegen.ts",
    "watch:graphql": "graphql-codegen --require dotenv/config --config codegen.ts"
  },
  "dependencies": {
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "autoprefixer": "10.4.14",
    "clsx": "^1.2.1",
    "dotenv": "^16.1.4",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "formik": "^2.4.2",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "next": "13.4.5",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "cross-env": "^7.0.3",
    "encoding": "^0.1.13",
    "prettier": "^2.8.8"
  }
}
